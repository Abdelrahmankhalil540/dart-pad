void main() {
  
  
  
  User username = User(name:"abdo",age:21,city:"desouk",student:Student(year:2030,grade:50),teach:Teach(subject:"ddddddd",level:10));
  print(username.student?.year);
  
//   Student studentname = Student(year:2020,grade:100);
