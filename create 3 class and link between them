class User{
  
  final String? name;
  final int? age ;
  final String? city;
  final Student? student;
  final Teach? teach;
  
  User({this.name,this.age,this.city,this.student,this.teach,});
  
}


class Student{
  final int? year ;
  final int? grade ;
  
  Student({this.year,this.grade});
}


class Teach{
  final String? subject ;
  final int? level ;
  
  Teach({this.subject,this.level});
}
